{
    "name": "bisonlab/sugarcrm-bundle",
    "type": "symfony-bundle",
    "description": "Bundle for simple access to the REST API of Sugar CRM 7 and up.",
    "keywords": ["symfony rest sugarcrm"],
    "homepage": "https://github.com/thomasez/SugarCrmBundle",
    "license": "GPLv2",
    "authors": [
        {
            "name": "Thomas Lundquist",
            "email": "github@bisonlab.no"
        }
    ],
    "require": {
        "symfony/framework-bundle": ">=2.2, <3.0",
        "spinegar/sugar7wrapper": "dev-master"
    },
    "suggest": {
        "redpilllinpro/nosql-bundle": "Another way to access Sugar with a different, but consistent API spanning more connectors than Sugar."
    },
    "autoload": {
        "psr-0": { "BisonLab\\SugarCrmBundle": "" }
    },
    "target-dir": "BisonLab/SugarCrmBundle"
}

